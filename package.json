    "eslint-plugin-react-refresh": "^0.4.20",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.5",
    "typescript": "^5.7.3",
